# Environment variables for YeetFlow Web App
# Copy this file to .env.local and fill in the actual values

# Authentication secret for better-auth
AUTH_SECRET=your_auth_secret_here

# Database connection URL
DATABASE_URL=your_database_url_here

# Worker service base URL
WORKER_BASE_URL=http://localhost:8000

# API token for worker authentication
WORKER_API_TOKEN=your_worker_api_token_here
